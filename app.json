{
	"expo": {
		"name": "Burner",
		"slug": "burner",
		"scheme": "acme",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/ss.png",
			"resizeMode": "contain",
			"backgroundColor": "#0D0D0C"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/icon.png",
				"backgroundColor": "#FFFFFF"
			}
		},
		"web": {
			"favicon": "./assets/icon.png",
			"bundler": "metro"
		}
	}
}
